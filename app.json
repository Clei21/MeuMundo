{
  "expo": {
    "name": "my-app",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LOOO.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/LOOO.png",
      "resizeMode": "contain",
      "backgroundColor": "#D4F0FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LOOO.png",
        "backgroundColor": "#D4F0FF"
      },
      "package": "com.cleid.myapp",
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "web": {
      "favicon": "./assets/LOOO.png"
    },
    "extra": {
      "eas": {
        "projectId": "613bdad8-65e0-48a8-bcdd-29c99a5e67da"
      }
    }
  }
}
